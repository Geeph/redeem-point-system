<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC 
      "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
      "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="family_relation" namespace="/family_relation" extends="struts-default">
		<!-- 切记：action name不能是一样的，否则会进不了Action -->
		<action name="familyRelation" class="com.integral.family.relationship.action.FamilyRelationAction" method="begin">
			 <result name="success">/jsp/family/family_relation.jsp</result>
			 <result name="input">/index.jsp</result>
		</action>
		<action name="familyRelationList" class="com.integral.family.relationship.action.FamilyRelationAction" method="familyRelationList">
			 <result name="success">/jsp/family/family_relation.jsp</result>
			 <result name="input">/index.jsp</result>
		</action>
		<action name="familyRelationEdit" class="com.integral.family.relationship.action.FamilyRelationAction" method="familyRelationEdit">
			 <result name="success">/jsp/family/family_relation.jsp</result>
			 <result name="input">/index.jsp</result>
		</action>
	</package>
</struts>