<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC 
      "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
      "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="family_member" namespace="/family_member" extends="struts-default">
		<!-- 切记：action name不能是一样的，否则会进不了Action -->
		<action name="familyMemberBegin" class="com.integral.family.member.action.FamilyMemberAction" method="begin">
			 <result name="success">/jsp/family/family.jsp</result>
			 <result name="input">/index.jsp</result>
		</action>
		<action name="familyMemberList" class="com.integral.family.member.action.FamilyMemberAction" method="familyMemberList">
			 <result name="success">/jsp/family/family.jsp</result>
			 <result name="input">/index.jsp</result>
		</action>
		<action name="familyMemberApply" class="com.integral.family.member.action.FamilyMemberAction" method="familyMemberApply">
			 <result name="success">/jsp/family/family.jsp</result>
			 <result name="input">/index.jsp</result>
		</action>
		<action name="familyMemberApplyProcess" class="com.integral.family.member.action.FamilyMemberAction" method="familyMemberApplyProcess">
			 <result name="success">/jsp/family/family.jsp</result>
			 <result name="input">/index.jsp</result>
		</action>
		<action name="familyMemberInfoEdit" class="com.integral.family.member.action.FamilyMemberAction" method="familyMemberInfoEdit">
			 <result name="success">/jsp/family/family.jsp</result>
			 <result name="input">/index.jsp</result>
		</action>
		<action name="familyMemberRemove" class="com.integral.family.member.action.FamilyMemberAction" method="familyMemberRemove">
			 <result name="success">/jsp/family/family.jsp</result>
			 <result name="input">/index.jsp</result>
		</action>
	</package>
</struts>